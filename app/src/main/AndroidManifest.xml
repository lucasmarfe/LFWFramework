<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.felipe.lucas.lfwframework">
    <application
        android:name=".StartApplications.StartApplicationForConfig"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Test_Screens.LoginScreen.LoginScreen">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
            <action android:name="android.intent.action.DEFAULT" />
        </intent-filter>
        </activity>
        <activity android:name=".Test_Screens.OperationScreens.ProductionOrderFormScreen"></activity>
        <activity android:name=".Screen.BaseConfigurationScreen"></activity>
        <activity android:name=".Test_Screens.OperationScreens.ProductionOrderResultsScreen"></activity>
        <activity android:name=".Test_Screens.OperationScreens.ProductionOrderDetailsScreen"></activity>
        <activity android:name=".Test_Screens.OperationScreens.ProductionExecutionFormScreen"></activity>
