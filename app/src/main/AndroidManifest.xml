        <activity android:name=".Test_Screens.OperationScreens.ProductionExecutionDetailsScreen"></activity>

        <activity android:name=".Test_Screens.QualityScreens.SampleFormScreen"></activity>
        <activity android:name=".Test_Screens.QualityScreens.SampleResultsScreen"></activity>
        <activity android:name=".Test_Screens.QualityScreens.SampleDetailsScreen"></activity>

        <activity android:name=".Test_Screens.Menu_Screens.MainMenuScreen"></activity>
        <activity android:name=".Test_Screens.Menu_Screens.CadastreMenuScreen"></activity>
        <activity android:name=".Test_Screens.Menu_Screens.OperationMenuScreen"></activity>
        <activity android:name=".Test_Screens.Menu_Screens.QualityMenuScreen"></activity>
        <activity android:name=".Test_Screens.Menu_Screens.ReportMenuScreen"></activity>
    </application>

</manifest>
